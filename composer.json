{
    "name": "sanpi/symbiose",
    "license": "MIT",
    "description": "Quickly create your next awesome PHP project based on symfony, pomm, atoum and behat",
    "require": {
        "php": ">=5.4",
        "symfony/symfony": "~2.5",
        "twig/extensions": "~1.0",
        "symfony/assetic-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~3.0.12",
        "pomm-project/foundation": "~2.0@dev",
        "pomm-project/pomm-bundle": "~2.0@dev",
        "pomm-project/model-manager": "~2.0@dev",
        "pomm-project/pomm-symfony-bridge": "~2.0@dev",
        "incenteev/composer-parameter-handler": "~2.0"
    },
    "require-dev": {
        "atoum/atoum": "~1.0",
        "pomm-project/cli": "~2.0@dev",
        "behatch/contexts": "~2.0",
        "behat/mink-extension": "~2.0",
        "behat/mink-goutte-driver": "*"
    },
    "autoload": {
        "psr-0": { "AppBundle": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Sanpi\\Test\\Unit\\": "tests/Unit" },
        "files": ["tests/Features/bootstrap/FeatureContext.php"]
    },
    "scripts": {
        "post-root-package-install": [
            "SymfonyStandard\\Composer::hookRootPackageInstall"
        ],
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
