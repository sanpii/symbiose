{
    "name": "sanpi/symbiose",
    "license": "MIT",
    "description": "Quickly create your next awesome PHP project based on symfony, pomm, atoum and behat",
    "require": {
        "php": ">=7.0",
        "pomm-project/pomm-bundle": "~2.0",
        "symfony/asset": "~3.0",
        "symfony/assetic-bundle": "~2.8",
        "symfony/config": "~3.0",
        "symfony/framework-bundle": "~3.0",
        "symfony/http-kernel": "~3.0",
        "symfony/monolog-bundle": "~3.0",
        "symfony/templating": "~3.0",
        "symfony/twig-bundle": "~3.0"
    },
    "require-dev": {
        "atoum/atoum": "~3.0",
        "behatch/contexts": "~2.6|~3.0",
        "behat/mink-goutte-driver": "*",
        "symfony/debug-bundle": "~3.0",
        "symfony/dotenv": "~3.0@dev",
        "symfony/web-profiler-bundle": "~3.0"
    },
    "autoload": {
        "psr-4": { "AppBundle\\": "src/" },
        "classmap": [ "app/AppKernel.php" ]
    },
    "autoload-dev": {
        "psr-4": { "Sanpi\\Test\\Unit\\": "tests/Unit" }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative"
    }
}
