{
    "name": "sanpi/symbiose",
    "license": "MIT",
    "description": "Quickly create your next awesome PHP project based on symfony, pomm, atoum and behat",
    "require": {
        "php": ">=7.0",
        "symfony/symfony": "~3.0",
        "symfony/assetic-bundle": "~2.7",
        "symfony/monolog-bundle": "~2.8",
        "pomm-project/pomm-bundle": "~2.0"
    },
    "require-dev": {
        "atoum/atoum": "~2.0",
        "behatch/contexts": "~2.6|~3.0",
        "behat/mink-goutte-driver": "*"
    },
    "autoload": {
        "psr-4": { "AppBundle\\": "src/" },
        "classmap": [ "app/AppKernel.php" ]
    },
    "autoload-dev": {
        "psr-4": { "Sanpi\\Test\\Unit\\": "tests/Unit" },
        "classmap": [ "tests/Features/bootstrap/FeatureContext.php" ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative"
    }
}
