{
    "name": "sanpi/symbiose",
    "license": "MIT",
    "description": "Quickly create your next awesome PHP project based on symfony, pomm, atoum and behat",
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0",
        "pomm-project/pomm-bundle": "~2.3",
        "symfony/asset": "~3.3",
        "symfony/assetic-bundle": "~2.8",
        "symfony/config": "~3.3",
        "symfony/flex": "~1.0",
        "symfony/framework-bundle": "~3.3",
        "symfony/http-kernel": "~3.3",
        "symfony/monolog-bundle": "~3.3",
        "symfony/templating": "~3.3",
        "symfony/twig-bundle": "~3.3"
    },
    "require-dev": {
        "atoum/atoum": "~3.0",
        "behatch/contexts": "~2.6|~3.0",
        "behat/mink-goutte-driver": "*",
        "symfony/debug-bundle": "~3.3",
        "symfony/dotenv": "~3.3",
        "symfony/web-profiler-bundle": "~3.3"
    },
    "autoload": {
        "psr-4": { "App\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Sanpi\\Test\\Unit\\": "tests/Unit" }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true"
        }
    }
}
