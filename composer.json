{
    "name": "sanpi/symbiose",
    "license": "MIT",
    "description": "Quickly create your next awesome PHP project based on symfony, pomm, atoum and behat",
    "require": {
        "php": ">=7.0",
        "pomm-project/pomm-bundle": "~2.4",
        "symfony/asset": "~4.0",
        "symfony/config": "~4.0",
        "symfony/framework-bundle": "~4.0",
        "symfony/http-kernel": "~4.0",
        "symfony/monolog-bundle": "~3.1",
        "symfony/templating": "~4.0",
        "symfony/twig-bundle": "~4.0",
        "symfony/yaml": "~4.0"
    },
    "require-dev": {
        "atoum/atoum": "~3.0",
        "behatch/contexts": "~2.6|~3.0",
        "behat/mink": "*@dev",
        "behat/mink-browserkit-driver": "*@dev",
        "behat/mink-goutte-driver": "*",
        "symfony/debug-bundle": "~4.0",
        "symfony/dotenv": "~4.0",
        "symfony/flex": "~1.0",
        "symfony/web-profiler-bundle": "~4.0"
    },
    "autoload": {
        "psr-4": { "App\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Sanpi\\Test\\Unit\\": "tests/Unit" }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true"
        }
    }
}
